<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_lancer_quote" model="ir.sequence">
            <field name="name">lancer Quote</field>
            <field name="code">lancer.quote</field>
            <field name="prefix">S%(year)s%(month)s%(day)s</field>
            <field eval="False" name="company_id"/>
            <field name="padding">2</field>
        </record>

        <record id="seq_lancer_version_handle" model="ir.sequence">
            <field name="name">lancer version handle</field>
            <field name="code">lancer.version.handle</field>
            <field name="prefix">H%(year)s%(month)s%(day)s</field>
            <field eval="False" name="company_id"/>
            <field name="padding">2</field>
        </record>

        <record id="seq_lancer_version_metal" model="ir.sequence">
            <field name="name">lancer version metal</field>
            <field name="code">lancer.version.metal</field>
            <field name="prefix">M%(year)s%(month)s%(day)s</field>
            <field eval="False" name="company_id"/>
            <field name="padding">2</field>
        </record>

    </data>
</odoo>
